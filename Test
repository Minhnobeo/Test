runTest("Large Loop Performance", function()
    local startTime = tick()
    for i = 1, 10000 do
        local _ = i * 2
    end
    local endTime = tick()
    return (endTime - startTime < 1) -- Should complete in under 1 second
end, "Hi·ªáu nƒÉng")
runTest("Table Operations", function()
    local testTable = {}
    for i = 1, 1000 do
        testTable[i] = "test" .. i
    end
    local found = false
    for i, v in pairs(testTable) do
        if v == "test500" then
            found = true
            break
        end
    end
    return found
end, "Hi·ªáu nƒÉng")
